      "Enter a number: 12\n",
      "12 is divisible by [1, 2, 3, 4, 6, 12]\n"
     ]
    }
   ],
   "source": [
    "num = input('Enter a number: ')\n",
    "\n",
    "NumRange = range(1, int(num)+1)\n",
    "\n",
    "divisors = []\n",
    "\n",
    "for x in NumRange:\n",
    "    if int(num) % x == 0:\n",
    "        divisors.append(x)\n",
    "        \n",
    "print(num, 'is divisible by', divisors)"
   ]
  }
